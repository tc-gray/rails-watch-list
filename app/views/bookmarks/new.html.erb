<div class="container" style="width: 40vw;">
<h1>Add a movie</h1>
<%= simple_form_for [@list, @bookmark] do |f| %>

  <div class="form-inputs">
    <%= f.association :movie %>
    <%= f.input :comment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-light' %>
  </div>
<% end %>
</div>
